{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Host Browser)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "web-server",
        "--web-port",
        "8080",
        "--web-hostname",
        "0.0.0.0"
      ],
      "cwd": "${workspaceFolder}",
      "env": {
        "FLUTTER_WEB_AUTO_DETECT": "true"
      }
    }
  ]
}
